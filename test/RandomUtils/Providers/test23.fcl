# Test the seeds service.
# 
# Policy:       preDefinedSeed
# Valid:        no (missing configuration for a module label)
# Will succeed: yes
# Purpose:      check unexpected module labels and engine instance names
#

#include "messageService.fcl"

# Give this job a name.
process_name : SeedTest01

# Start form an empty source
source :
{
  module_type : EmptyEvent
  maxEvents : 1
}

services :
{
  message : @local::mf_interactive
  RandomNumberGenerator: {}

  LArSeedService: {
    policy            : "preDefinedSeed"
    verbosity         :     2
    endOfJobSummary   :  false

    stest01 : 3

    stest02 : {
      a : 5
      b : 7
    }
  } # LArSeedService

}

physics :
{
  analyzers: {
    stest01 : {
      module_type : SeedTest01
      module_name : stest01
      testMode      : 1
      instanceNames : [ "" ]
    }

    stestUnexpected : {
      module_type : SeedTest01
      module_name : stestUnexpected
      testMode      : 1
      instanceNames : [ "a", "b" ]
      expectedErrors: 4
    }

  }

  e1 : [stest01, stestUnexpected]

  end_paths      : [e1]

}
